package DB;



public class Tables {

	public static abstract class BaseColumns {
		public static final String _ID = "id";
	}
	
	public static abstract class HumanResource extends BaseColumns {
		public static final String TABLE_NAME = "human-resource";
		public static final String COLUMN_NAME_FIRSTNAME = "firstname";
		public static final String COLUMN_NAME_LASTNAME = "lastname";
        public static final String COLUMN_NAME_MELINUM = "melinum";
        public static final String COLUMN_NAME_EMPLOYEEID = "employeeid";
        public static final String COLUMN_NAME_STARTDATE = "startdate";
        public static final String COLUMN_NAME_LEVEL = "level";
        public static final String COLUMN_NAME_DEPARTMENT = "department";
	}
}
